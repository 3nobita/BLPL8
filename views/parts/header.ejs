    <script>
        const popup = document.getElementById('popupForm');
        const closeBtn = document.querySelector('.close-btn');

        // Get all elements with the class 'openFormBtn'
        const openBtns = document.querySelectorAll('.openFormBtn');

        // Add click event listener to each 'openFormBtn'
        openBtns.forEach(btn => {
            btn.onclick = function (event) {
                event.preventDefault(); // Prevent the default link behavior
                if (popup) {
                    popup.style.display = 'block';
                }
            }
        });

        // Ensure that closeBtn exists before adding the onclick event listener
        if (closeBtn) {
            closeBtn.onclick = function () {
                if (popup) {
                    popup.style.display = 'none';
                }
            }
        }

        // When the user clicks anywhere outside of the popup, close it
        window.onclick = function (event) {
            if (popup && event.target === popup) {
                popup.style.display = 'none';
            }
        }
    </script>

    <script>
        let currentStep = 1;

        function showStep(step) {
            const steps = document.querySelectorAll('.form-step');
            const icons = document.querySelectorAll('.step-icon');
            const progressBarFill = document.getElementById('progress-bar-fill');

            steps.forEach((stepDiv) => {
                stepDiv.classList.remove('active');
            });
            icons.forEach((icon, index) => {
                if (index < step - 1) {
                    icon.classList.add('completed');
                } else if (index === step - 1) {
                    icon.classList.add('active');
                } else {
                    icon.classList.remove('completed', 'active');
                }
            });

            document.getElementById(`step${step}`).classList.add('active');

            const percentage = ((step - 1) / 4) * 100;
            progressBarFill.style.width = `${percentage}%`;
        }

        function nextStep() {
            if (currentStep < 5) {
                currentStep++;
                showStep(currentStep);
            }
        }

        function prevStep() {
            if (currentStep > 1) {
                currentStep--;
                showStep(currentStep);
            }
        }

        // Initialize the form to show the first step
        showStep(currentStep);
    </script>
